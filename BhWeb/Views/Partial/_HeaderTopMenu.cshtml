@model BH.Models.PublicViewModel
<div class="header-top">
    <div class="container pt-1 pb-1">
        <div class="header-left">
            <ul class="top-menu top-link-menu d-none d-md-block">
                <li>
                    <a href="#">Links</a>
                    <ul>
                        <li><a href="tel:#"><i class="icon-phone"></i>Call: @Model.CompanyDetails.CompanyMobileNo</a></li>
                    </ul>
                </li>
            </ul><!-- End .top-menu -->
        </div><!-- End .header-left -->
        <div class="header-right">
            <div class="social-icons social-icons-color">
                <a href="@Model.CompanyDetails.FacebookLink" class="social-icon social-facebook" title="Facebook" target="_blank"><i class="icon-facebook-f"></i></a>
                <a href="@Model.CompanyDetails.InstagramLink" class="social-icon social-twitter" title="Twitter" target="_blank"><i class="icon-twitter"></i></a>
                <a href="@Model.CompanyDetails.InstagramLink" class="social-icon social-pinterest" title="Instagram" target="_blank"><i class="icon-pinterest-p"></i></a>
                <a href="@Model.CompanyDetails.LinkdinLink" class="social-icon social-instagram" title="Pinterest" target="_blank"><i class="icon-instagram"></i></a>
            </div><!-- End .soial-icons -->
            <ul class="top-menu top-link-menu">
                <li>
                    <a href="#">Links</a>
                    <ul>
                        <li><a href="#signin-modal" data-toggle="modal"><i class="icon-user"></i>Login</a></li>
                    </ul>
                </li>
            </ul><!-- End .top-menu -->

        </div><!-- End .header-right -->
    </div>
</div>


